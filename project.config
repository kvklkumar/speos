[project]
	description = Symphony Persistent emc Opentext Sapient
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/kvklkumar
[access "refs/for/refs/*"]
	read = group user/kvklkumar
	push = group user/kvklkumar
	label-Code-Review = -2..+2 group user/kvklkumar
	label-Verified = -1..+1 group user/kvklkumar
	rebase = group user/kvklkumar
	removeReviewer = group user/kvklkumar
	submit = group user/kvklkumar
[access "refs/heads/*"]
	read = group user/kvklkumar
	create = group user/kvklkumar
	push = +force group user/kvklkumar
	pushMerge = group user/kvklkumar
[access "refs/tags/*"]
	push = group user/kvklkumar
	push = +force group user/kvklkumar
[receive]
	requireChangeId = true
	createNewChangeForAllNotInTarget = true
	rejectImplicitMerges = true
[change]
	privateByDefault = true
[reviewer]
	enableByEmail = true
[submit]
	mergeContent = true
	matchAuthorToCommitterDate = true
